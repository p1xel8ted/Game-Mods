    <Reference>
      <Private>False</Private>
    </Reference>
    <ProjectReference>
      <Private>False</Private>
    </ProjectReference>
    <PackageReference>
      <Private>False</Private>
    </PackageReference>
  </ItemDefinitionGroup>

